 *
 */


#ifndef _PCA9685_H_
#define _PCA9685_H_
#include "stm32f1xx_hal.h"

/**************PCA9685寄存器定义*********************/
#define PCA9685_MODE1                           0x00H
#define PCA9685_MODE2                           0x01H
#define PCA9685_SUBADR1                       0x02H
#define PCA9685_SUBADR2                       0x03H
#define PCA9685_SUBADR3                       0x04H
#define PCA9685_ALLCALLADR                 0x05H
#define  PCA9685_LED0_ON_L                    0x06H
#define  PCA9685_LED0_ON_H                   0x07H
#endif
